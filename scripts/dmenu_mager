#/bin/bash

DMENU='dmenu -fn 'TerminessTTF-9' -i -l 10 -x 350 -w 700 -y 250 -nb #000000 -nf #999999 -sb #FFFFFF -sf #31658C'
choice=$(echo -e "calf\nxfceterm\nkrita\ntodo\nemacs\nconkyfetch\nmlterm\nxtermsixel\nterminator\nkitty\ntermite\nalacritty\nidm\ndeepchallenge\nayudark\nayumirage\nayulight\ngruvboxdark\ngruvboxlight\ndarktheme\nlighttheme\nhitamputih\nputihhitam\nmerahputih\nputihmerah\ndarkdefault\nlightdefault\nescape\nkillmpg123\nalarm\naudacious\nsterminal\nlibreoffice\ngimp\nlogoutdwm\nrefreshdwm\ncolor\ncarla\njackstart\njackstop\nwebcam\nclipdel\nytop\nhtop\nweb\nmkvtool\nmixxx\nkillmixxx\nkillmpv\nmenu\ncatia\nxp\nkeyboard\nmousepad\nwide\nnormal\nlock\nlogout\nviter\nnetwork\nreboot\nshutdown\nsuspend\nterminal\nkonsol\nrefresh\nlist\nfilemanager\nplay\naria\nwallpaperfill\nwallpaperscale\ncalc\ntypora\ntag\ngammy\ntouchpad\nconvert\nfile\nscriptedit\nbuku\nrecord\nstop\nmonitor\nkill\nvim\nsteam\ntvs\npron\nvideo\ndrawing\nfceux\nkalkulator\ntelegram\nqutebrowser\ncadence\nolive\nfilm\npavmixer\npavucontrol\nmute\nvolmax\nvolhalf\nnes\nsnes\nbright30\nbright50\nbright100\nplayer\nmusic\npause\nstoprec\nnext\nprev\nminimize\nmaximize\nfullscreen\nmpvcontrol\nmpvcontrolnext\nmpvcontrolprev\nmpvplaylist\nclose\nscreenshotshadowborder\nscreenshotshadowborderarea\nscreenshotshadowborderdelay\nscreenshotsimple\nscreenshotsimplearea\nscreenshotsimpledelay\nscreenshot\nscreenshotarea\nscreenshotdelay\nyoutube\ncompton\nxeyes\nvokoscreen\nudiskie" | $DMENU)

case "$choice" in
      calf) calfjackhost eq12 ! ;;
      audacious) env LIBGL_ALWAYS_SOFTWARE=1 audacious  ;;
      libreoffice) libreoffice ;;
      sterminal) /usr/local/bin/st ;;
      terminator) terminator ;;
      xfceterm) xfce4-terminal ;;
      xtermsixel) xterm -ti v340;;
      mlterm) mlterm;;
      kitty) kitty ;;
      termite) termite ;;
      alacritty) alacritty ;;
      krita) krita ;;
      idm) pyidm ;;
      emacs) emacs ;;
      gimp) gimp ;;
      logoutdwm) kill -TERM $(pidof -s dwm) ;;
      refreshdwm) kill -HUP $(pidof -s dwm) ;;
      color) gcolor2 ;;
      alarm) ~/Documents/scripts/alarm ;;
      conkyfetch) ~/Documents/scripts/conkyfetch ;;
      todo) ~/Documents/scripts/dmenu_todo ;;
      killmpg123) ~/Documents/scripts/killmpg123 ;;
      oldtheme) wal -f ~/Documents/colorscheme/hijau.txt ;;
      deepchallenge) wal -f ~/Documents/colorscheme/terminal.sexy.txt ;;
      ayudark) wal -f ~/Documents/colorscheme/ayudarktest.txt ;;
      ayumirage) wal -f ~/Documents/colorscheme/ayumiragetest.txt ;;
      ayulight) wal -f ~/Documents/colorscheme/ayulighttest.txt ;;
      gruvboxdark) wal -f ~/Documents/colorscheme/gruvboxnabildark.txt ;;
      gruvboxlight) wal -f ~/Documents/colorscheme/gruvboxhardlight.txt ;;
      darktheme) wal -f ~/Documents/colorscheme/baskerville-lostwoods.txt ;;
      lighttheme) wal -f ~/Documents/colorscheme/baskerville-ivorylight.txt ;;
      hitamputih) wal -f ~/Documents/colorscheme/hitamputih.txt ;;
      putihhitam) wal -f ~/Documents/colorscheme/putihhitam.txt ;;
      merahputih) wal -f ~/Documents/colorscheme/merahputih.txt ;;
      putihmerah) wal -f ~/Documents/colorscheme/putihmerah.txt ;;
      darkdefault) wal -f ~/Documents/colorscheme/darkdefault.txt ;;
      lightdefault) wal -f ~/Documents/colorscheme/lightdefault.txt ;;
      escape) ~/Documents/scripts/escape ;;
      jackstart) ~/Documents/scripts/start-jack.sh ;;
      jackstop) ~/Documents/scripts/stop-jack.sh ;;
      carla) carla ~/Documents/carla/test.carxp -n;;
      webcam) ~/Documents/scripts/webcam ;;
      wallpaperfill) ~/Documents/scripts/gantifill ;;
      wallpaperscale) ~/Documents/scripts/gantiscale ;;
      web) ~/Documents/scripts/xterms -e w3m ~/.w3m/bookmark.html ;;
      mixxx) env LIBGL_ALWAYS_SOFTWARE=1 mixxx ;;
      killmixxx) pkill mixxx ;;
      killmpv) pkill mpv ;;
      menu) ~/Documents/scripts/j4menu  ;;
      mkvtool) mkvtoolnix-gui  ;;
      keyboard) onboard  ;;
      mousepad) mousepad  ;;
      catia) catia  ;;
      mecha-buckling-on) buckle -g 50  ;;
      wide) xrandr --output eDP1 --scale-from 1440x900 --panning 1440x900 ;;
      normal) xrandr --output eDP1 --scale-from 1366x768 --panning 1366x768 ;;
      refresh) i3-msg restart ;;
      list) ~/Documents/scripts/list ;;
      viter) python3 ~/Documents/bangunapp/viter/viter.py ;;
      calc) libreoffice --calc ;;
      typora) typora ;;
      tag) easytag ;;
      gammy) gammy ;;
      terminal) xterm ;;
      konsol) ppsspp ;;
      filemanager) pcmanfm ;;
      player) smplayer ;;
      music) ~/Documents/scripts/mpds ;;
      clipdel) ~/Documents/scripts/clipdel ;;
      aria) ~/Documents/scripts/dmenu_aria2 ;;
      touchpad) ~/Documents/scripts/touchpad.sh devonoff ;;
      convert) ~/Documents/scripts/converthb ;;
      file) ~/Documents/scripts/dmenufm ;;
      scriptedit) ~/Documents/scripts/confmenu ;;
      buku) ~/Documents/scripts/books ;;
      record) ~/Documents/scripts/dmenurecord ;;
      stoprec) ~/Documents/scripts/stoprec ;;
      monitor) ~/Documents/scripts/dmenu_display ;;
      kill) ~/Documents/scripts/dmenu-kill-process ;;
      vim) xterm -e vim ;;
      ytop) st -e ytop -b ;;
      htop) st -e htop -t -u damez --sort-key=PERCENT_MEM  ;;
      steam) steam ;;
      tvs)~/Documents/scripts/tvs ;;
      pron)~/Documents/scripts/sans ;;
      video)~/Documents/scripts/mpvinfinite ;;
      drawing) drawing ;;
      fceux) ~/Documents/scripts/emu ;;
      kalkulator) ~/Documents/scripts/itung ;;
      telegram) telegram-desktop ;;
      qutebrowser) qutebrowser --backend webengine ;;
      cadence) cadence ;;
      olive) olive-editor ;;
      film) ~/Documents/scripts/film ;;
      pavmixer) st -e pulsemixer ;;
      pavucontrol) pavucontrol ;;
      mute) pactl set-sink-mute 0 toggle ;;
      volmax) pamixer --set-volume 100 ;;
      volhalf) pamixer --set-volume 50 ;;
      nes) fceux ;;
      snes) snes9x-gtk ;;
      bright30) light -S 30 ;;
      bright50) light -S 50 ;;
      bright100)light -S 100 ;;
      play) ~/Documents/scripts/mpds ;;
      pause) mpc toggle ;;
      stop) mpc stop ;;
      next) mpc next ;;
      prev) mpc prev ;;
      minimize) i3-msg move scratchpad ;;
      maximize) ~/Documents/scripts/hide ;;
      close) i3-msg kill & ;;
      fullscreen) i3-msg fullscreen ;;
      mpvcontrolnext) echo playlist-next | socat - ~/.config/mpv/mpvsocket ;;
      mpvcontrolprev) echo playlist-prev | socat - ~/.config/mpv/mpvsocket ;;
      mpvcontrol) echo cycle pause | socat - ~/.config/mpv/mpvsocket ;;
      mpvplaylist) ~/Documents/scripts/mpvplaylist  ;;
      screenshot) ~/Documents/scripts/screenshot.sh & ;;
      screenshotarea) ~/Documents/scripts/screenshot.sh -s & ;;
      screenshotdelay) ~/Documents/scripts/scrots & ;;
      screenshotsimple) ~/Documents/scripts/screenshotsimpleborder.sh & ;;
      screenshotsimplearea) ~/Documents/scripts/screenshotsimpleborder.sh -s & ;;
      screenshotsimpledelay) ~/Documents/scripts/scrotssimple & ;;
      screenshotshadowborder) ~/Documents/scripts/screenshotshadowonly.sh & ;;
      screenshotshadowborderarea) ~/Documents/scripts/screenshotshadowonly.sh -s & ;;
      screenshotshadowborderdelay) ~/Documents/scripts/scrotsshadowborder & ;;
      youtube) ~/Documents/scripts/ytplay & ;;
      compton) ~/Documents/scripts/xcomptog & ;;
      vokoscreen) vokoscreen & ;;
      xeyes) xeyes -fg black & ;;
      udiskie) ~/Documents/scripts/udiskie-dmenu & ;;
      lock) betterlockscreen -l & ;;
      network) networkmanager_dmenu  & ;;
      logout) i3-msg exit & ;;
      reboot) exec systemctl reboot & ;;
      shutdown) exec systemctl -i poweroff & ;;
      suspend) exec systemctl suspend & ;;
esac

